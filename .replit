[nix]
channel = "stable-24_05"
packages = ["libuuid", "cairo", "pango", "giflib", "libjpeg", "pixman", "pkg-config", "ffmpeg", "yt-dlp", "tesseract"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
